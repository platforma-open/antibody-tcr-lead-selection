{
  "name": "@platforma-open/milaboratories.top-antibodies.model",
  "version": "1.15.5",
  "description": "Block model",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js",
      "sources": "./src/index.ts"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "ts-builder build --target block-model && block-tools build-model",
    "watch": "ts-builder build --target block-model --watch",
    "type-check": "ts-builder types --target block-model",
    "lint": "eslint .",
    "test": "vitest",
    "do-pack": "shx rm -f *.tgz && pnpm pack && shx mv *.tgz package.tgz"
  },
  "dependencies": {
    "@platforma-sdk/model": "catalog:",
    "@milaboratories/graph-maker": "catalog:",
    "@milaboratories/strings": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/ts-builder": "catalog:",
    "@milaboratories/ts-configs": "catalog:",
    "@platforma-sdk/block-tools": "catalog:",
    "@platforma-sdk/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@platforma-sdk/ui-vue": "catalog:"
  }
}
